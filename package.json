{
  "name": "naver",
  "version": "1.0.0",
  "description": "",
  "author": "daechan-jo",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest build --webpack --webpackPath webpack-hmr.config.cjs",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:pm2": "pm2 start dist/main.js --name naver --env production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@daechanjo/log": "^1.0.0",
    "@daechanjo/models": "^1.0.22",
    "@daechanjo/playwright": "^1.2.2",
    "@daechanjo/rabbitmq": "^1.0.1",
    "@eslint/eslintrc": "^3.3.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/typeorm": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "amqp-connection-manager": "^4.1.14",
    "axios": "^1.8.3",
    "bcrypt": "^5.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "moment-timezone": "^0.5.48",
    "pg": "^8.14.1",
    "prettier": "^3.5.3",
    "run-script-webpack-plugin": "^0.2.2",
    "typeorm-transactional": "^0.5.0",
    "webpack-node-externals": "^3.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.11.10",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.9",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/models(|/.*)$": "<rootDir>/libs/models/src/$1"
    }
  }
}
